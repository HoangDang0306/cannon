<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  

<hibernate-mapping>
	<class name="model.Player" table="PLAYER_INFO">
	<id name="id" column="id" type="int">
		<generator class="assigned"></generator>
	</id>

	<property name="name" column="name" type="string"></property>
	<property name="password" column="password" type="string"></property>
	<property name="created" column="created" type="string"></property>
	<property name="updated" column="updated" type="string"></property>
	<property name="match" column="match" type="int"></property>

	</class>
</hibernate-mapping>