<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  

<hibernate-mapping>
	<class name="model.Match" table="match_info">
	<id name="id" column="id" type="int">
		<generator class="assigned"></generator>
	</id>

	<property name="playerId" column="player_id" type="int"></property>
	<property name="currentSide" column="current_side" type="int"></property>
	<property name="playerHp" column="player_hp" type="int"></property>
	<property name="enemyHp" column="enemy_hp" type="int"></property>
	<property name="status" column="status" type="int"></property>

	</class>
</hibernate-mapping>