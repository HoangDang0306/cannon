<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  

<hibernate-mapping>
	<class name="model.EnemyBoard" table="enemy_board_info">
	<id name="id" column="id" type="int">
		<generator class="assigned"></generator>
	</id>

	<property name="cannon1" column="cannon_1" type="short"></property>
	<property name="cannon2" column="cannon_2" type="short"></property>
	<property name="cannon3" column="cannon_3" type="short"></property>
	<property name="cannon4" column="cannon_4" type="short"></property>
	<property name="cannon5" column="cannon_5" type="short"></property>
	<property name="cannon6" column="cannon_6" type="short"></property>
	<property name="cannon7" column="cannon_7" type="short"></property>

	</class>
</hibernate-mapping>